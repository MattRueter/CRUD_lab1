html 
    //variables used in this template assigned to same name variables here for readability.
    - var title= title 
    - var heading= heading
    - var route= route
    - var method= method
    - var buttonName= buttonName
    - var message= message
    - var names= names
    - var notFoundMessage= notFoundMessage
    include mixins/nav_buttons
    include mixins/search_delete_form
    include mixins/add_update_form
    head
        title= title 
        style
            include ./styles.css
    body
        h1= heading
        +nav_buttons()
        if method==="GET" 
            +search_delete_form()
        else
            +add_update_form()        
        
        p= message         
        - var names = names 
        ul 
            #names
            if names.length > 0
                each val in names
                    li= val.name + " --- Nationality: " + val.nationality + " --- DOB: " + val.dob
        p= notFoundMessage
